<?php 
namespace MisClases;

class Remera{
	protected $id;
	protected $nombre;
	protected $descripcion;
	protected $link;
	protected $precio;
	protected $image;
	protected $talle;
	protected $color;

	public function _construct($id, $nombre, $descripcion, $link, $image, $talle, $color, $precio){
		$this->id=$id;
		$this->nombre=$nombre;
		$this->descripcion=$descripcion;
		$this->link=$link;
		$this->image=$image;
		$this->talle=$talle;
		$this->color=$color;
		$this->precio=$precio;
	}
}
?>